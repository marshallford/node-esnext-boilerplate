{
  "comments": false,
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["babel-root-import", { "rootPathSuffix": "src" }]
  ],
  "env": {
    "production": {
      "plugins": [
        ["babel-root-import", { "rootPathSuffix": "dist" }]
      ],
    },
  }
}
